<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GradientProgress">
        <attr name="gradientStart" format="color" />
        <attr name="gradientEnd" format="color" />
        <attr name="backCircleColor" format="color" />
        <attr name="minAngle" format="float" />
        <attr name="maxSweep" format="float" />
        <attr name="lineWidth" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RecognitionTextSwitcher">
        <attr name="initText" format="reference" />
    </declare-styleable>
    <declare-styleable name="HorizontalProgress">
        <attr name="horizontalLabelTextSize" format="dimension" />
        <attr name="horizontalValueTextSize" format="dimension" />
        <attr name="horizontalProgressHeight" format="dimension" />
        <attr name="horizontalProgressWidth" format="dimension" />
        <attr name="horizontalProgressMargin" format="dimension" />
        <attr name="horizontalLabel" format="string" />
    </declare-styleable>
    <declare-styleable name="CircleProgress">
        <attr name="circleValueTextSize" format="dimension" />
        <attr name="circleProgressSize" format="dimension" />
        <attr name="circleValueBottomMargin" format="dimension" />
        <attr name="circleLabelBottomMargin" format="dimension" />
        <attr name="circleProgressBottomPadding" format="dimension" />
        <attr name="circleLabel" format="string" />
    </declare-styleable>
</resources>
